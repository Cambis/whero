{
  "name": "whero",
  "version": "0.12.0",
  "private": true,
  "browserslist": [
    "last 2 major versions"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "yarn codegen && next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:fix": "yarn lint:scripts:fix && yarn lint:styles:fix",
    "lint:scripts": "eslint ./src/**/**.{ts,tsx}",
    "lint:scripts:fix": "eslint --fix ./src/**/**.{ts,tsx}",
    "lint:styles": "stylelint ./src/**/**.{css,scss}",
    "lint:styles:fix": "stylelint --fix ./src/**/**.{css,scss}",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "prettier:graphql": "prettier --parser graphql --print-width 100 --write ./src/graphql/**/*.gql",
    "prettier:migrations": "prettier --parser espree --print-width 100 --write ./migrations/**.js",
    "prettier:all": "yarn prettier:graphql && yarn prettier:migrations",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/nextjs": "^6.19.7",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "datocms-client": "^3.5.19",
    "formidable": "^2.0.1",
    "formik": "^2.2.9",
    "graphql": "^16.4.0",
    "graphql-request": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "next": "^12.1.6",
    "next-sitemap": "^2.5.20",
    "nodemailer": "^6.7.4",
    "react": "^18.1.0",
    "react-datocms": "^3.0.12",
    "react-dom": "^18.1.0",
    "react-google-recaptcha": "^2.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-graphql-request": "^4.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@tailwindcss/forms": "^0.5.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/node": "17.0.31",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-google-recaptcha": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  },
  "resolutions": {
    "@types/react": "18.0.8"
  }
}
